{
  "expo": {
    "name": "dyad-react-native-expo-template",
    "slug": "dyad-react-native-expo-template",
    "plugins": ["expo-router"],
    "experiments": { "typedRoutes": false },
    "web": {
      "name": "Hurry Coffee Admin",
      "shortName": "Hurry Admin",
      "description": "Bảng quản lý dành cho Admin và Nhân viên của Hurry Coffee.",
      "lang": "vi-VN",
      "themeColor": "#73509c",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "favicon": "./public/icons/icon-192x192.png",
      "icon": "./public/icons/icon-512x512.png",
      "splash": {
        "image": "./assets/images/logo-app-PWA.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent"
      },
      "bundler": "metro"
    }
  }
}